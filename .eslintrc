{
  "env": {
    "browser": 1
  },
  "globals": {
    "chrome": 1,
    'UserVoice': 1
  },
 
  "rules": {
	'no-multi-spaces': 0,
	'no-multiple-empty-lines': 0,
    'eqeqeq': 0,
    'quotes': [2, "single"],
    'strict': 0,
    'comma-spacing': 0,
    'indent': 0,
    'linebreak-style': 0,
    'max-len': 0,
    'no-mixed-spaces-and-tabs': 0,
    'space-comment': 0,
    'spaced-comment': 0,
	'no-multiple-empty-lines': 0,
	'no-negated-condition': 0,
	'no-trailing-spaces': 0,
	'no-unused-vars': 0,
	'padded-blocks': 0,
	'require-jsdoc': 0,
	'space-after-keywords': 0,
	'space-before-blocks': 0,
	'comma-dangle':0,
	'default-case':0,
	'no-inline-comments':0,
	'key-spacing':0,
	'space-infix-ops':0,
	'camelcase':0,
	'brace-style':0,
	'no-multi-str': 0,
	'object-curly-spacing': 0,
	'no-warning-comments':0,
	'space-before-keywords':0,
	'space-in-parens':0,
	'one-var':0,
	'no-else-return':0,
	'no-else-return':0,
	'yoda':0,
	'quote-props':0,
	'consistent-return':0,
	'space-before-function-paren':0,
	'guard-for-in':0,
	'space-unary-ops':0,
	'no-lonely-if':0,
	'no-extend-native':0,
	'quotes':0,
	'semi-spacing':0,
	'semi':0,
	'space-return-throw-case':0,
	'no-implicit-coercion':0,
	'curly':0,
	'no-eq-null':0,
	'no-new-func':0,
	'new-parens':0,
	'eol-last':0,
	'valid-jsdoc':0,

'no-unused-expressions':0,
'new-cap':0,
'no-lone-blocks':0,
'no-undef':0,
 'block-scoped-var':0,
 'no-use-before-define':0,
 'no-redeclare':0, 
 'no-loop-func':0, 
 'no-cond-assign':0, 
 'no-sequences':0, 
 'no-new':0, 
'no-nested-ternary':0, 
'wrap-iife':0, 

'no-void':0, 
'no-return-assign':0, 
'no-throw-literal':0, 
'no-empty-label':0, 
'no-labels':0, 
'no-label-var':0, 
'no-useless-call':0, 
'no-extra-semi':0, 
'arrow-parens':1,
'computed-property-spacing':0,
'operator-assignment':0,
'no-useless-concat':0,
'no-native-reassign':0,
  }
}